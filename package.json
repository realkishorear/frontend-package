{
  "name": "jgd-fe-cli",
  "version": "1.0.0",
  "description": "A production-ready React + TypeScript project generator CLI with multiple templates, bundlers, and configurations",
  "keywords": [
    "react",
    "typescript",
    "cli",
    "generator",
    "vite",
    "webpack",
    "tailwind",
    "scaffold"
  ],
  "author": "",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "jgd-fe": "./bin/cli.js"
  },
  "files": [
    "bin",
    "dist",
    "src",
    "prompts",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "build": "tsc && npm run build:copy",
    "build:copy": "node scripts/copy-generator.js",
    "dev": "tsc --watch",
    "postinstall": "node bin/postinstall.js || echo 'Build skipped - will build on first run'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "prepublishOnly": "npm run build && npm run lint && npm run typecheck"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.1",
    "inquirer": "^9.2.12",
    "terminal-image": "^4.1.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.19.30",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/realkishorear/frontend-package.git"
  },
  "bugs": {
    "url": "https://github.com/realkishorear/frontend-package/issues"
  },
  "homepage": "https://github.com/realkishorear/frontend-package#readme"
}
